{
  "manifest_version": 2,
  "name": "Git Revive",
  "version": "1.0.0",
  "default_locale": "en",
  "description": "Bring GitHub back to life.",
  "icons": {},
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*",
        "*://*.github.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "author": "Henrikh Kantuni, Shahen Kosyan"
}