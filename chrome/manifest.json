{
  "manifest_version": 2,
  "name": "Git Revive",
  "version": "1.0.0",
  "description": "Bring GitHub back to life.",
  "icons": {
    "16": "icons/icon-16x16.png",
    "48": "icons/icon-48x48.png",
    "128": "icons/icon-128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*",
        "*://*.github.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "homepage_url": "https://github.com/bruntouchables/GitRevive",
  "author": "Henrikh Kantuni, Shahen Kosyan"
}